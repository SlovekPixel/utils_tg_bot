generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id          Int         @id @default(autoincrement())
  user_id     Int         @unique
  username    String?
  admin       Boolean     @default(false)
  access      Boolean     @default(true)
  roles       String      @default("[]")
  created_at  DateTime    @default(now())
}